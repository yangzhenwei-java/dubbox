<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.1.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.1.xsd
		http://code.alibabatech.com/schema/dubbo  http://fk.test.beebank.com/dubbo.xsd " >
	<context:property-placeholder location="zookeeper.properties" />
	<dubbo:application name="param_callback_provider_demo"/>
	<dubbo:registry address="${zookeeper.address}"/>
	<dubbo:protocol name="dubbo"  port="20889" host="${host}"/>
	 <dubbo:monitor protocol="registry" />
	<dubbo:service interface="com.github.service.UserService" ref="userServiceImpl">
		<dubbo:method name="batchAddUsers">
	        <dubbo:argument index="0" callback="true" />
	    </dubbo:method>
	</dubbo:service>
	<bean id="userServiceImpl" class="com.github.impl.UserServiceImpl"/>

</beans>